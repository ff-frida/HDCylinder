<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆角柱状图-->
    <declare-styleable name="CylinderView">
        <!--正常柱形宽度百分比-->
        <attr name="cy_barWidthRatio" format="float" />
        <!--选中的柱形宽度百分比-->
        <attr name="cy_selectBarWidthRatio" format="float" />
        <!--柱形间距宽度百分比-->
        <attr name="cy_sepRatio" format="float" />
        <!--柱形距离横坐标点娿距离百分比-->
        <attr name="cy_marginTextRatio" format="float" />
        <!--画板右边距百分比-->
        <attr name="cy_marginRightRatio" format="float" />
        <!--画板左边距百分比-->
        <attr name="cy_marginLeftRatio" format="float" />
        <!--画板上边距百分比-->
        <attr name="cy_marginTopRatio" format="float" />
        <!--画板下边距百分比-->
        <attr name="cy_marginBottomRatio" format="float" />
        <!--文字的大小百分比-->
        <attr name="cy_textRatio" format="float" />
        <!--尖部三角形边长百分比-->
        <attr name="cy_triangleRatio" format="float" />
        <!--动画持续时间-->
        <attr name="cy_animationTime" format="integer" />
    </declare-styleable>
</resources>
